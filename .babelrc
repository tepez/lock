{
  "plugins": [
    "version-inline"
  ],
  "presets": [
    [
      "es2015",
      {
        "loose": true,
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "npm": {
      "presets": [
        [
          "es2015",
          {
            "loose": true,
            "modules": "commonjs"
          }
        ],
        "stage-0",
        "react"
      ]
    }
  }
}